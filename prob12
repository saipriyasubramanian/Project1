#include <stdio.h>
#include <math.h>

int main()
{
    long int n,max,sum,counter,i =2,j;
    n = 0;max = 0;sum = 1;
    while(n <= 500)
    {
        n = 0;
        sum += i;
        i++;
     for(j = 1;j <= sqrt(sum); j++)
     {
        if(sum%j == 0)
            n = n+2;
     }
       
    }
     printf("%ld\n",sum);
	return 0;
}
